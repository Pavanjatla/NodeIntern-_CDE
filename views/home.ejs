<%- include ("./partials/head.ejs")  %> 

<div class="blogs content">
   
  
  <% if(blogs.length > 0) { %> <%blogs.forEach(blog => { %>
  <a class="" href="/blogs/<%=blog._id%>"><%=blog.title %> </a>
  <h3><%= blog.snippet %></h3>
  <h3><%= blog.body %></h3>
  <% });%> <%} else {%>
  <h1>No blog to display</h1>
  <% } %>
</div>


